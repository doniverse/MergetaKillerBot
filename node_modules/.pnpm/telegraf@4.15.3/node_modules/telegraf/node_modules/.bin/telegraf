#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Hp/Documents/New folder/Coding/TG bots/anti-mergeta/node_modules/.pnpm/telegraf@4.15.3/node_modules/telegraf/lib/node_modules:/mnt/c/Users/Hp/Documents/New folder/Coding/TG bots/anti-mergeta/node_modules/.pnpm/telegraf@4.15.3/node_modules/telegraf/node_modules:/mnt/c/Users/Hp/Documents/New folder/Coding/TG bots/anti-mergeta/node_modules/.pnpm/telegraf@4.15.3/node_modules:/mnt/c/Users/Hp/Documents/New folder/Coding/TG bots/anti-mergeta/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Hp/Documents/New folder/Coding/TG bots/anti-mergeta/node_modules/.pnpm/telegraf@4.15.3/node_modules/telegraf/lib/node_modules:/mnt/c/Users/Hp/Documents/New folder/Coding/TG bots/anti-mergeta/node_modules/.pnpm/telegraf@4.15.3/node_modules/telegraf/node_modules:/mnt/c/Users/Hp/Documents/New folder/Coding/TG bots/anti-mergeta/node_modules/.pnpm/telegraf@4.15.3/node_modules:/mnt/c/Users/Hp/Documents/New folder/Coding/TG bots/anti-mergeta/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.mjs" "$@"
else
  exec node  "$basedir/../../lib/cli.mjs" "$@"
fi
